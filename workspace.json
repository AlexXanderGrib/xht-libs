{
  "version": 2,
  "projects": {
    "safe-math": {
      "root": "packages/safe-math",
      "sourceRoot": "packages/safe-math/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/safe-math/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/safe-math"
          ],
          "options": {
            "jestConfig": "packages/safe-math/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/safe-math",
            "tsConfig": "packages/safe-math/tsconfig.lib.json",
            "packageJson": "packages/safe-math/package.json",
            "main": "packages/safe-math/src/index.ts",
            "assets": [
              "packages/safe-math/*.md"
            ]
          }
        }
      }
    },
    "sign": {
      "root": "packages/sign",
      "sourceRoot": "packages/sign/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/sign/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/sign"
          ],
          "options": {
            "jestConfig": "packages/sign/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/sign",
            "tsConfig": "packages/sign/tsconfig.lib.json",
            "packageJson": "packages/sign/package.json",
            "main": "packages/sign/src/index.ts",
            "assets": [
              "packages/sign/*.md"
            ]
          }
        }
      }
    },
    "bablo": {
      "root": "packages/bablo",
      "sourceRoot": "packages/bablo/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/bablo/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/packages/bablo"
          ],
          "options": {
            "jestConfig": "packages/bablo/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/packages/bablo",
            "tsConfig": "packages/bablo/tsconfig.lib.json",
            "packageJson": "packages/bablo/package.json",
            "main": "packages/bablo/src/index.ts",
            "assets": [
              "packages/bablo/*.md"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}